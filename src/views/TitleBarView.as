package views{	import controllers.Controller;	import flash.display.MovieClip;	import models.Model;	public class TitleBarView extends View	{		protected var _myIcon:MovieClip=null;		protected var _padding:Number=10;				public function TitleBarView(amodel:Model, acontroller:Controller)		{			trace("TitleBarView.View");			super(amodel, acontroller);			refreshView();		}		private function clearView():void		{			if (_myIcon != null)			{				removeChild(_myIcon);				_myIcon = null;			}		}		public override function refreshView():void		{			clearView();			_myIcon = new IconLogo();//replace with your Icon			addChild(_myIcon);			_myIcon.height = _stage.stageHeight / 10;			_myIcon.scaleX = _myIcon.scaleY;		}		public override function destroyView():void		{			clearView();		}	}}